            = image_tag "images/catering.svg"

      %article
        .content
          %p
            = image_tag "images/shake-it-off.svg"
          %dl.count
            %dd
              %span#odometer.odometer 0
              Number of Sonos speakers in the office
            %dd
              %span#odometer2.odometer 0
              Times we danced to“Shake it Off” by Taylor Swift
            %dd
              %span#odometer3.odometer 0
              Number of days that song was available on Spotify
          %header.header-title
            Consumed Daily
            %span{style: "width: 330px"}
          =image_tag "images/consumed-daily.svg", class: "animated consumed-daily this"
          =image_tag "images/consumed-dail-bars.svg", class: "animated consumed-daily next"

  %section.section.office
    .container
      %aside{style: "padding-top: 10%"}
        %p
          =image_tag "images/conferences-header.svg", class: "heading"
          =image_tag "images/conferences-stats.svg"
